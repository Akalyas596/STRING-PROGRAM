public class  {
    public static void main(String[] args){
        String s1="a1bc2d3e4";
        int count=0;
       for(int i=0;i<s1.length()-1;i++){
           char ch=s1.charAt(i);
           if(ch=='a'||ch=='e'||ch=='i'||ch=='0'||ch=='u'){

               count++;

               System.out.println(s1.charAt(i));
           }
       }
        System.out.println(count);
    }
}
